{
	"name": "typescript-starter",
	"version": "1.0.0",
	"private": true,
	"description": "My personal TypeScript starter template.",
	"keywords": [
		"typescript",
		"starter"
	],
	"bugs": {
		"url": "https://github.com/jonahsnider/typescript-starter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/typescript-starter.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"type": "module",
	"exports": "./tsc_output/src/index.js",
	"main": "./tsc_output/src/index.js",
	"scripts": {
		"build": "tsc",
		"lint": "xo",
		"style": "prettier --check .",
		"test": "ava",
		"test:coverage": "c8 --reporter lcov --reporter text-summary --100 --include 'src/**/*.ts' --exclude 'src/types/**/*.ts' --all ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.3.0",
		"@tsconfig/node18": "18.2.2",
		"@tsconfig/strictest": "2.0.2",
		"@types/node": "20.10.4",
		"ava": "5.3.1",
		"c8": "8.0.1",
		"prettier": "3.1.0",
		"tsx": "3.14.0",
		"typescript": "5.3.3",
		"xo": "0.56.0"
	},
	"packageManager": "yarn@3.7.0"
}
